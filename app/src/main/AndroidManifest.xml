            android:name=".ViewDrafts"
            android:label="Generate Time Table">
            <intent-filter>
                <action android:name="com.proj.timetable.ViewDrafts" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>



        <activity
            android:name=".CreatePdf"
            android:label="Generate Time Table">
            <intent-filter>
                <action android:name="com.proj.timetable.CreatePdf" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".GeneratedTimeTable"
            android:label="Generate Time Table">
            <intent-filter>
                <action android:name="com.proj.timetable.GeneratedTimeTable" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


