            android:name=".CreateTimeTablePDF"
            android:label="Generate Time Table">
            <intent-filter>
                <action android:name="com.proj.timetable.CreateTimeTablePDF" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

